<?xml version="1.0" encoding="UTF-8"?>
<form>
	
	<field
		name="id"
		type="text"
		default="0"
		label="JGLOBAL_FIELD_ID_LABEL"
		description ="JGLOBAL_FIELD_ID_DESC"
		class="readonly"
		readonly="true"	/>
	
	<field
		name="venue"
		type="text"
		default=""
		label="COM_REDEVENT_VENUE"
		description ="COM_REDEVENT_VENUE" 
		required="true" 
		class="inputbox" />
	
	<field
		name="alias"
		type="text"
		default=""
		label="COM_REDEVENT_Alias"
		description ="COM_REDEVENT_Alias"
		class="inputbox" />
	
	<field
		name="published"
		type="list"
		class="inputbox"
		default="1"
		size="1"
		label="JSTATUS"
		description="JFIELD_PUBLISHED_DESC">
		<option
			value="1">
			JPUBLISHED</option>
		<option
			value="0">
			JUNPUBLISHED</option>
	</field>
		
	<field
		name="description"
		type="editor"
		label="COM_REDEVENT_DESCRIPTION"
		description="COM_REDEVENT_DESCRIPTION"
		class="inputbox"
		filter="safehtml"
		buttons="true"
		hide="readmore,pagebreak"/>
		
	<field
		name="locimage"
		type="media"
		label="COM_REDEVENT_CHOOSE_IMAGE"
		description="COM_REDEVENT_CHOOSE_IMAGE"
		class="inputbox"
		directory="redevent/categories"/>
		
</form>
